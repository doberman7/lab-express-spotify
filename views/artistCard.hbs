  <div class="card-body">
    <h5 class="card-title">{{name}}</h5>
    <form action="/artistSearch" method="GET">

      <div class="artistSearch">
        <label for="albums/{{id}}"></label>
        <input type="text" name="albums/{{id}}" class="form-control" id="albums/{{id}}" placeholder="" value="">


      </div>
      <br>
